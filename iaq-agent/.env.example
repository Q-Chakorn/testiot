# IAQ Sensor Agent Configuration
# Copy this file to .env and modify the values as needed

# CSV Data Configuration
# Multi-file mode (preferred) - reads all CSV files in directory
CSV_DIRECTORY_PATH=../sample_data
CSV_FILE_PATTERN=*.csv
CSV_READING_MODE=chronological

# Single-file mode (legacy) - uncomment to use single file
# CSV_DATA_PATH=../sample_data/sample_iaq_data_Room101.csv
# DEVICE_ID=room_101

# Scheduler Configuration
SEND_INTERVAL=5
BATCH_SIZE=1
MAX_RETRIES=3
RETRY_DELAY=2
ENABLE_REPLAY=true
START_FROM_BEGINNING=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
STATS_INTERVAL=60

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=sensor_data
RABBITMQ_EXCHANGE=
RABBITMQ_ROUTING_KEY=
RABBITMQ_DURABLE=true
RABBITMQ_CONNECTION_TIMEOUT=30
RABBITMQ_HEARTBEAT=600
RABBITMQ_RETRY_DELAY=5
RABBITMQ_MAX_RETRIES=3